{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "landing/nuxt.config.ts",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "landing/.output/public"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "landing/.output/public/$1"
    }
  ]
}
